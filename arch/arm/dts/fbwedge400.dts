/dts-v1/;

#include "ast2500-fboss.dtsi"

/ {
	aliases {
		eth0 = &mac1;
	};
};

&wdt1 {
	boot2nd = <1>;
};

&mac1 {
	status = "okay";
	phy-mode = "rgmii";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_mac2link_default &pinctrl_mdio2_default>;
	phy-handle = <&ethphy1 &ethphy2>;
	#address-cells = <1>;
	#size-cells = <0>;
	ethphy1: ethernet-phy@1 {
		reg = <0>;
	};
	ethphy2: ethernet-phy@2 {
		reg = <13>;
	};
};

#include <dt-bindings/gpio/aspeed-gpio.h>
/ {
	board_id {
		compatible = "board_id";
		board-id-gpios = <&gpio ASPEED_GPIO(G, 4) GPIO_ACTIVE_HIGH>,
		                 <&gpio ASPEED_GPIO(G, 5) GPIO_ACTIVE_HIGH>,
		                 <&gpio ASPEED_GPIO(G, 6) GPIO_ACTIVE_HIGH>,
		                 <&gpio ASPEED_GPIO(G, 7) GPIO_ACTIVE_HIGH>,
				 <&gpio ASPEED_GPIO(N, 6) GPIO_ACTIVE_HIGH>,
				 <&gpio ASPEED_GPIO(N, 7) GPIO_ACTIVE_HIGH>;
	};
};
